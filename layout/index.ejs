<%- include('_partials/header.ejs') %>
<body>
<% if (title) { %>
    <h2><%= title %></h2>
<% } %>

<% if (author) { %>
    <h2><%= author %></h2>
<% } %>
<ul>
    <% articleList.forEach(function(article) { %>
        <li><a href="<%= article.link %>"><%= article.meta.title %></a></li>
    <% }); %>
</ul>
</body>
</html>

